    {
        "id": "799b2e111de9dfb8",
        "type": "function",
        "z": "6c57a4257b2aeef5",
        "name": "detectar mudan√ßa",
        "func": "// Estado atual das vagas vindo do function 1\nlet atual = msg.payload;\n\n// Pega o estado anterior salvo\nlet anterior = global.get(\"vagas_anterior\") || {};\n\n// Verifica se houve mudan√ßa em qualquer uma das vagas\nlet mudou =\n    atual.vaga1 !== anterior.vaga1 ||\n    atual.vaga2 !== anterior.vaga2 ||\n    atual.vaga3 !== anterior.vaga3;\n\n// Atualiza o estado anterior\nglobal.set(\"vagas_anterior\", atual);\n\n// Se n√£o mudou, n√£o envia nada\nif (!mudou) {\n    return null;\n}\n\n// -----------------------------\n// 1) MENSAGEM PARA TELEGRAM\n// -----------------------------\nlet msgTelegram = {\n    payload: {\n        chatId: 8053871352,\n        type: \"message\",\n        content:\n            \"üöó *Status das Vagas Atualizado*\\n\\n\" +\n            `Vaga 1: *${atual.vaga1}*\\n` +\n            `Vaga 2: *${atual.vaga2}*\\n` +\n            `Vaga 3: *${atual.vaga3}*`\n    }\n};\n\n// -----------------------------\n// 2) MENSAGEM PARA INFLUXDB\n// -----------------------------\nlet msgInflux = {\n    measurement: \"status\",\n    payload: {\n        vaga1: atual.vaga1,\n        vaga2: atual.vaga2,\n        vaga3: atual.vaga3\n    }\n};\n\n// ENVIA DE UMA S√ì VEZ (2 sa√≠das)\nreturn [msgTelegram, msgInflux];\n",
